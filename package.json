{
  "name": "test",
  "version": "4.0.0",
  "description": "Node.js 23's node:test, as an npm package",
  "license": "MIT",
  "repository": "nodejs/node-core-test",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "test": "for i in test/parallel/*; do node $i; done"
  },
  "bin": {
    "node-test": "./lib/cli.js"
  },
  "imports": {
    "#internal/*": "./lib/internal/*.js",
    "#lib/*": "./lib/*.js",
    "#node:test": "./lib/test.js",
    "#node:test/reporters": "./lib/reporters.js"
  },
  "dependencies": {
    "frozen-fruit": "^0.0.2",
    "minimatch": "9.0.5"
  }
}
